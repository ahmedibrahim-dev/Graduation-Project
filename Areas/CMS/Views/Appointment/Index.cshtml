
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/CMS/Views/Shared/_Layout.cshtml";
}
					<!-- Page Header -->
<div class="page-header">
    <div class="row">
        <div class="col-sm-12">
            <h3 class="page-title">Appointments</h3>
            <ul class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")">Dashboard</a></li>
                <li class="breadcrumb-item active">Appointments</li>
            </ul>
        </div>
    </div>
</div>
<!-- /Page Header -->
<div class="row">
    <div class="col-md-12">

        <!-- Recent Orders -->
        <div class="card">
            <div class="card-body">
                <div class="table-responsive">
                    <table class="datatable table table-hover table-center mb-0">
                        <thead>
                            <tr>
                                <th>Doctor Name</th>
                                <th>Client Name</th>
                                <th>Apointment Time</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    <h2 class="table-avatar">
                                        <a class="avatar avatar-sm mr-2"><img class="avatar-img rounded-circle" src="~/Areas/CMS/assets//img/doctors/doctor-thumb-01.jpg" alt="User Image"></a>
                                        <a>Dr. Ruby Perrin</a>
                                    </h2>
                                </td>
                                <td>
                                    <h2 class="table-avatar">
                                        <a class="avatar avatar-sm mr-2"><img class="avatar-img rounded-circle" src="~/Areas/CMS/assets//img/patients/patient1.jpg" alt="User Image"></a>
                                        <a>Charlene Reed </a>
                                    </h2>
                                </td>
                                <td>9 Nov 2021 <span class="text-primary d-block">11.00 AM - 11.15 AM</span></td>
                                <td>
                                    <div class="status-toggle">
                                        <input type="checkbox" id="status_1" class="check" checked>
                                        <label for="status_1" class="checktoggle">checkbox</label>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <h2 class="table-avatar">
                                        <a class="avatar avatar-sm mr-2"><img class="avatar-img rounded-circle" src="~/Areas/CMS/assets//img/doctors/doctor-thumb-02.jpg" alt="User Image"></a>
                                        <a>Dr. Darren Elder</a>
                                    </h2>
                                </td>
                                <td>
                                    <h2 class="table-avatar">
                                        <a class="avatar avatar-sm mr-2"><img class="avatar-img rounded-circle" src="~/Areas/CMS/assets//img/patients/patient2.jpg" alt="User Image"></a>
                                        <a>Travis Trimble </a>
                                    </h2>
                                </td>

                                <td>5 Nov 2021 <span class="text-primary d-block">11.00 AM - 11.35 AM</span></td>
                                <td>
                                    <div class="status-toggle">
                                        <input type="checkbox" id="status_2" class="check" checked>
                                        <label for="status_2" class="checktoggle">checkbox</label>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- /Recent Orders -->

    </div>
</div>

