<!DOCTYPE html>
<html lang="en">

<!-- Mirrored from dreamguys.co.in/demo/doccure/admin/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Sat, 30 Nov 2019 04:12:20 GMT -->
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <title>EYE CARE | Admin Dashboard</title>

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/Images/eye.png">

    <!-- Bootstrap CSS -->

    <link rel="stylesheet" href="~/Areas/CMS/assets/css/bootstrap.min.css">

    <!-- Fontawesome CSS -->
    <link rel="stylesheet" href="~/Areas/CMS/assets/css/font-awesome.min.css">

    <!-- Feathericon CSS -->
    <link rel="stylesheet" href="~/Areas/CMS/assets/css/feathericon.min.css">

    <link rel="stylesheet" href="~/Areas/CMS/assets/plugins/morris/morris.css">

    <!-- Main CSS -->
    <link rel="stylesheet" href="~/Areas/CMS/assets/css/style.css">
    <link href="~/Content/toastr.min.css" rel="stylesheet" />
    <!--[if lt IE 9]>
        <script src="~/Areas/CMS/assets/js/html5shiv.min.js"></script>
        <script src="~/Areas/CMS/assets/js/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <!-- Main Wrapper -->
    <div class="main-wrapper">
        <!-- Header -->
        <div class="header">

            <!-- Logo -->
            <div class="header-left">
                <a href="@Url.Action("Index","Home")" class="logo" style="color:#0a3d62; font-size:25px; font-weight:bold;">
                    <img src="~/Images/eye.png" alt="Logo">
                    EYE CARE
                </a>
                <a href="@Url.Action("Index","Home")" class="logo logo-small">
                    <img src="~/Images/eye.png" alt="Logo" width="30" height="30">
                </a>
            </div>
            <!-- /Logo -->

            <a href="@Url.Action("Index","Home")" id="toggle_btn">
                <i class="fe fe-text-align-left"></i>
            </a>
            <!-- Mobile Menu Toggle -->
            <a class="mobile_btn" id="mobile_btn">
                <i class="fa fa-bars"></i>
            </a>
            <!-- /Mobile Menu Toggle -->
            <!-- Header Right Menu -->
            <ul class="nav user-menu">
                <!-- Notifications -->
                <li class="nav-item dropdown noti-dropdown">
                    <a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown">
                        <i class="fe fe-bell"></i> <span class="badge badge-pill">1</span>
                    </a>
                    <div class="dropdown-menu notifications">
                        <div class="topnav-dropdown-header">
                            <span class="notification-title">Notifications</span>
                            <a href="javascript:void(0)" class="clear-noti"> Clear All </a>
                        </div>
                        <div class="noti-content">
                            <ul class="notification-list">
                                <li class="notification-message">
                                    <a href="#">
                                        <div class="media">
                                            <span class="avatar avatar-sm">
                                                <img class="avatar-img rounded-circle" alt="User Image" src="~/Areas/CMS/assets/img/doctors/doctor-thumb-01.jpg">
                                            </span>
                                            <div class="media-body">
                                                <p class="noti-details"><span class="noti-title">Dr. Ruby Perrin</span> Schedule <span class="noti-title">her appointment</span></p>
                                                <p class="noti-time"><span class="notification-time">4 mins ago</span></p>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                                <li class="notification-message">
                                    <a href="#">
                                        <div class="media">
                                            <span class="avatar avatar-sm">
                                                <img class="avatar-img rounded-circle" alt="User Image" src="~/Areas/CMS/assets/img/patients/patient1.jpg">
                                            </span>
                                            <div class="media-body">
                                                <p class="noti-details"><span class="noti-title">Charlene Reed</span> has booked her appointment to <span class="noti-title">Dr. Ruby Perrin</span></p>
                                                <p class="noti-time"><span class="notification-time">6 mins ago</span></p>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                                <li class="notification-message">
                                    <a href="#">
                                        <div class="media">
                                            <span class="avatar avatar-sm">
                                                <img class="avatar-img rounded-circle" alt="User Image" src="~/Areas/CMS/assets/img/patients/patient2.jpg">
                                            </span>
                                            <div class="media-body">
                                                <p class="noti-details"><span class="noti-title">Travis Trimble</span> sent a amount of $210 for his <span class="noti-title">appointment</span></p>
                                                <p class="noti-time"><span class="notification-time">8 mins ago</span></p>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                                <li class="notification-message">
                                    <a href="#">
                                        <div class="media">
                                            <span class="avatar avatar-sm">
                                                <img class="avatar-img rounded-circle" alt="User Image" src="~/Areas/CMS/assets/img/patients/patient3.jpg">
                                            </span>
                                            <div class="media-body">
                                                <p class="noti-details"><span class="noti-title">Carl Kelly</span> send a message <span class="noti-title"> to his doctor</span></p>
                                                <p class="noti-time"><span class="notification-time">12 mins ago</span></p>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="topnav-dropdown-footer">
                            <a href="#">View all Notifications</a>
                        </div>
                    </div>
                </li>
                <!-- /Notifications -->
                <!-- User Menu -->
                <li class="nav-item dropdown has-arrow">
                    <a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown">
                        <span class="user-img"><img class="rounded-circle" src="~/Areas/CMS/assets/img/profiles/avatar-02.jpg" width="31" alt="Ryan Taylor"></span>
                    </a>
                    <div class="dropdown-menu">
                        <div class="user-header">
                            <div class="avatar avatar-sm">
                                <img src="~/Areas/CMS/assets/img/profiles/avatar-02.jpg" alt="User Image" class="avatar-img rounded-circle">
                            </div>
                            <div class="user-text">
                                <h6>Ryan Taylor</h6>
                                <p class="text-muted mb-0">Administrator</p>
                            </div>
                        </div>
                        <a class="dropdown-item" href="@Url.Action("Index","Profile")">My Profile</a>
                        <a class="dropdown-item" href="@Url.Action("LogIn","Account")">Logout</a>
                    </div>
                </li>
                <!-- /User Menu -->

            </ul>
            <!-- /Header Right Menu -->

        </div>
        <!-- /Header -->
        <!-- Sidebar -->
        <div class="sidebar" id="sidebar">
            <div class="sidebar-inner slimscroll">
                <div id="sidebar-menu" class="sidebar-menu">
                    <ul>                     
                        <li class="menu-title">
                            <span>Main</span>
                        </li>
                        <li class="li">

                            <a href="@Url.Action("Index","Home")"><i class="fe fe-home"></i> <span>Dashboard</span></a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Appointment")"><i class="fe fe-layout"></i><span>Appointments</span></a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Orders")"><i class="fa fa-shopping-bag"></i> <span>Orders</span></a>
                        </li>
                        <li class="menu-title">
                            <span>Users</span>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Doctor")"><i class="fe fe-user-plus"></i> <span>Doctors</span></a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Seller")"><i class="fe fe-user-plus"></i> <span>Sellers</span></a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Client")"><i class="fe fe-user"></i> <span>Clients</span></a>
                        </li>
                        <li class="menu-title">
                            <span>Pages</span>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Profile")"><i class="fe fe-user-plus"></i> <span>Profile</span></a>
                        </li>
                        <li class="menu-title">
                            <span>View Website</span>
                        </li>
                        <li>
                            <a href="~/Home/Index"><img src="~/Images/eye.png" alt="Logo" width="30" height="30"><span>Eye Care</span></a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- /Sidebar -->
        <!-- Page Wrapper -->
        <div class="page-wrapper">

            <div class="content container-fluid">
                @RenderBody()

            </div>
        </div>
        <!-- /Page Wrapper -->

    </div>
    <!-- /Main Wrapper -->
    <!-- jQuery -->
    <script src="~/Areas/CMS/assets/js/jquery-3.2.1.min.js"></script>

    <!-- Bootstrap Core JS -->
    <script src="~/Areas/CMS/assets/js/popper.min.js"></script>
    <script src="~/Areas/CMS/assets/js/bootstrap.min.js"></script>

    <!-- Slimscroll JS -->
    <script src="~/Areas/CMS/assets/plugins/slimscroll/jquery.slimscroll.min.js"></script>

    <script src="~/Areas/CMS/assets/plugins/raphael/raphael.min.js"></script>
    <script src="~/Areas/CMS/assets/plugins/morris/morris.min.js"></script>
    <script src="~/Areas/CMS/assets/js/chart.morris.js"></script>

    <!-- Custom JS -->
    <script src="~/Areas/CMS/assets/js/script.js"></script>

    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/toastr.min.js"></script>
    @RenderSection("scripts", required: false)
</body>

<!-- Mirrored from dreamguys.co.in/demo/doccure/admin/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Sat, 30 Nov 2019 04:12:34 GMT -->
</html>



